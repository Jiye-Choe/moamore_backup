<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
  
<mapper namespace="teamMember">
	<insert id="insertOne" parameterType="teamMemberDTO">
		insert into team_member values(#{team_no},#{id},#{nickname},#{saving})	
	</insert>
	<select id="selectAllByTeamNo" parameterType="int" resultType="teamMemberDTO">
		select * from team_member where team_no = #{value} ORDER BY saving DESC	
	</select>
	<select id="deleteTeamMemberAll" parameterType="int">
		delete from team_member where team_no = #{value}
	</select>
</mapper>